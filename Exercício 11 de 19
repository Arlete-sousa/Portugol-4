programa
{
     inteiro num1, num2, num3, num4, num5, num6, num7, num8, num9, num10, soma, total, result
	
	funcao inicio()
	{
	escreva("Escreva 10 números: ")
	leia(num1, num2, num3, num4, num5, num6, num7, num8, num9, num10)
	
	limpa()
	
	soma = (num1+num2+num3+num4+num5+num6+num7+num8+num9+num10)
	total = (10)
		
	result = (soma/total)
	escreva("Valor da média aritmética: ", result)
	}
}
