programa
{
	inteiro num1, num2
	funcao inicio()
	{
	escreva("Digite o primeiro número: ")
	leia(num1)
	escreva("Digite o segundo número: ")
	leia(num2)
		
	se (num1 > num2){
	escreva("O primeiro é maior")
	}
	senao{
	se(num1 < num2){
	escreva("o segundo é maior")
	}
	senao{
	escreva("Números iguais")
			}
		}
	}
}
