programa
{
	
	funcao inicio()
	{
	inteiro x, i

	faca{
	escreva("Digite o número 5: ")
	leia(x)
	}enquanto(x <= 0)

	para(i = 0; i <= 10; i++){
	escreva(5, " x ", i, " = ", x * i, "\n")
	    }
	}
}
